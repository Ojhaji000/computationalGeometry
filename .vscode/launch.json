{
  "version": "0.2.0",
  "configurations": [
    {
        "name": "Line-Triangle Intersection",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/1.line-triangle-intersection/run1.LineTriangleExe",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}/1.line-triangle-intersection",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Set Disassembly Flavor to Intel",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
        ]
    },
    {
        "name": "Star Polygon",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/2.star-polygon/run2.StarPolygonExe",
